processes:
  - name: statsd_injector
    executable: /var/vcap/packages/statsd_injector/statsd-injector
    args:
    - --statsd-host
    -  "<%= p("statsd_injector.statsd_host") %>"
    - --statsd-port
    - "<%= p("statsd_injector.statsd_port") %>"
    - --metron-port
    - "<%= p("statsd_injector.metron_port") %>"
    - --ca
    - "/var/vcap/jobs/statsd_injector/certs/ca.crt"
    - --cert
    - "/var/vcap/jobs/statsd_injector/certs/statsd_injector.crt"
    - --key
    - "/var/vcap/jobs/statsd_injector/certs/statsd_injector.key"
